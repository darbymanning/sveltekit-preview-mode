{
  "name": "hygraph",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev --port 4000 --open",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte --check --ignore-path=../../.prettierignore . && TIMING=1 eslint \"src\"",
    "format": "prettier --plugin prettier-plugin-svelte --write --ignore-path=../../.prettierignore ."
  },
  "dependencies": {
    "schema": "workspace:*",
    "sveltekit-preview-mode": "workspace:*",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "latest",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@urql/svelte": "^4.0.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.0",
    "graphql": "^16.8.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "type": "module"
}
