{
  "name": "hygraph",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev --port 4001 --open",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte --check --ignore-path=../../.prettierignore . && TIMING=1 eslint \"src\"",
    "format": "prettier --plugin prettier-plugin-svelte --write --ignore-path=../../.prettierignore .",
    "prepare": "svelte-kit sync"
  },
  "dependencies": {
    "schema": "workspace:*",
    "sveltekit-preview-mode": "workspace:*",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "5.5.2",
    "typescript": "5.7.2",
    "@sveltejs/kit": "2.10.1",
    "svelte": "5.11.2",
    "@sveltejs/package": "2.3.7",
    "@sveltejs/adapter-auto": "3.3.1",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "vite": "6.0.3",
    "tslib": "2.8.1",
    "eslint": "9.16.0",
    "@types/eslint": "9.6.1",
    "typescript-eslint": "8.18.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.46.1",
    "globals": "15.13.0",
    "svelte-check": "4.1.1",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.2",
    "@urql/svelte": "4.2.2"
  },
  "type": "module"
}
