{
  "name": "sveltekit-preview-mode-sveltekit-preview-mode",
  "version": "0.1.8",
  "description": "CMS Preview Mode for SvelteKit",
  "repository": "https://github.com/darbymanning/sveltekit-preview-mode.git",
  "author": "Darby Manning <me@darbymanning.com>",
  "keywords": [
    "svelte",
    "sveltekit",
    "cms",
    "preview",
    "mode",
    "hygraph"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "svelte-package -w",
    "build": "vite build && svelte-package && cp ../../README.md ./README.md",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "prepare": "svelte-kit sync"
  },
  "peerDependencies": {
    "@sveltejs/kit": ">=2.0.0",
    "svelte": ">=5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "5.5.2",
    "@sveltejs/kit": "latest",
    "@sveltejs/package": "2.3.7",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "cookie": "1.0.2",
    "svelte": "5.11.2",
    "svelte-check": "4.1.1",
    "tslib": "2.8.1",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
